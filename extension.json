{
    "id": "ibm.devops.services.pipeline.appscan.dynamic.builder",
    "version": 1,
    "name_key": "ExtName",
    "desc_key": "ExtDesc",
    "extension_type": "Build",
    "message_key": "ExtMessage",
    "inputs": [
        {
            "type": "Bluemix",
            "inclusion" : "always"
        }
    ],
    "params": [
        {
            "name": "SETUP_SERVICE_SPACE",
            "type": "Text",
            "required": "true",
            "default_value": "false",
            "label_key": "ACCEPT_KEY",
            "desc_key": "ACCEPT_DESC"
        },
        {
            "name": "SUBMISSION_NAME",
            "type": "Text",
            "required": "false",
            "label_key": "SUBMISSION_NAME_KEY",
            "desc_key": "SUBMISSION_NAME_DESC"
        },
        {
            "name": "TARGET",
            "type": "Text",
            "required": "false",
            "default_value": "/",
            "label_key": "TARGET_KEY",
            "desc_key": "TARGET_DESC"
        }
    ],
    "outputs": [
        {
            "type": "Artifacts",
            "inclusion" : "always"
        }
    ],
    "execution": {
        "type": "JenkinsDocker",
        "shell": "#!/bin/bash
export IMAGE_NAME=#IMAGE_NAME#
export USE_CACHED_LAYERS=#USE_CACHED_LAYERS#
set +x 
set +e 
. $EXT_DIR/_init.sh

echo -e \"${red}To be implemented${no_color}\""
    }
    },
    "project": "https://hub.jazz.net/project/alchemy/Alchemy-Ostanes",
    "project_example": "https://hub.jazz.net/pipeline/rjminsha/ice-pipeline_demo",
    "project_contact": "rjminsha@us.ibm.com"
}
